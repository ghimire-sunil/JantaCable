<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_container_movement_tree" model="ir.ui.view">
        <field name="name">container.movement.list</field>
        <field name="model">container.movement</field>
        <field name="priority" eval="50"/>
        <field name="arch" type="xml">
            <list string="Container Movement">
                <field name='date'/>
                <field name="particulars"/>
                <field name="reference"/>
                <field name="location_id"/>
                <field name="location_dest_id"/>
                <field name="quantity"/>
                <field name="unit_of_measure"/>
                <field name="status"/>
                <field name="balance"/>
        </list>
        </field>
    </record>

</odoo>