<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="attendance_form_view_inherited" model="ir.ui.view">
        <field name="name">attendance.form.view.inherited</field>
        <field name="model">hr.attendance</field>
        <field name="inherit_id" ref="hr_attendance.hr_attendance_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='check_out']" position="after">
                <label for="image_1920" string="Image"/>
                <field name="image_1920" widget="image" class="oe_inline" nolabel="1" />
            </xpath>
        </field>
    </record>

</odoo>