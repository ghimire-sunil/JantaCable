<?xml version="1.0" encoding='UTF-8'?>
<odoo>
    <data noupdate="0">
            <record forcecreate="True" id="ir_cron_update_order_taken" model="ir.cron">
                <field name="name">Customer: Update Order taken</field>
                <field eval="True" name="active" />
                <!-- <field name="user_id" ref="base.user_admin" /> -->
                <field name="interval_number">1</field>
                <field name="interval_type">days</field>
                <field name="model_id" ref="base.model_res_partner" />
                <field name="state">code</field>
                <field name="code">model.sudo()._cron_ordered_today()</field>
                <field name="nextcall" eval="(DateTime.now() + relativedelta(days=1, hour=0, minute=0))"/>
            </record>

            <record forcecreate="True" id="ir_cron_update_visited_today" model="ir.cron">
                <field name="name">Customer: Update Visited Today</field>
                <field eval="True" name="active" />
                <field name="user_id" ref="base.user_admin" />
                <field name="interval_number">1</field>
                <field name="interval_type">days</field>
                <field name="model_id" ref="base.model_res_partner" />
                <field name="state">code</field>
                <field name="code">model._cron_visited_today()</field>
                <field name="nextcall" eval="(DateTime.now() + relativedelta(days=1, hour=0, minute=0))"/>
            </record>

            <record forcecreate="True" id="ir_cron_update_payment_taken" model="ir.cron">
                <field name="name">Customer: Update Payment Taken</field>
                <field eval="True" name="active" />
                <field name="user_id" ref="base.user_admin" />
                <field name="interval_number">1</field>
                <field name="interval_type">days</field>
                <field name="model_id" ref="base.model_res_partner" />
                <field name="state">code</field>
                <field name="code">model._cron_payment_taken()</field>
                <field name="nextcall" eval="(DateTime.now() + relativedelta(days=1, hour=0, minute=0))"/>
            </record>
    </data>
</odoo>
