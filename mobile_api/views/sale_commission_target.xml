<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="sale_commission_plan_form_view_inherit" model="ir.ui.view">
        <field name="name">sale.commission.plan.form.view.inherit</field>
        <field name="model">sale.commission.plan</field>
        <field name="inherit_id" ref="sale_commission.sale_commission_plan_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='target_ids'][1]/list[1]/field[@name='amount']" position="after">
                <field name="store_target" />
            </xpath>
        </field>
    </record>

    <record id="sale_commission_report_list_view_inherit" model="ir.ui.view">
        <field name="name">sale.commission.report.list.view.inherit</field>
        <field name="model">sale.commission.report</field>
        <field name="inherit_id" ref="sale_commission.sale_commission_report_view_list" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='target_amount']" position="after">
                <field name="store_target" />
            </xpath>
        </field>
    </record>

</odoo>