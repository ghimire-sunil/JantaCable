<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="distributor_achievement_form_view" model="ir.ui.view">
            <field name="name">distributor.achievement.form</field>
            <field name="model">distributor.achievement.wizard</field>
            <field name="arch" type="xml">
                <form string="Distributor Achievement">
                    <group col="4" string="General Information">
                        <field name="from_date" />
                        <field name="to_date" />
                        <field name="user_ids" widget="many2many_tags"/>
                    </group>
                    <footer>
                        <button name="action_open_window" string="Open Achievement" type="object" class="btn-primary" data-hotkey="q"/>
                        <button string="Cancel" class="btn-secondary" special="cancel" data-hotkey="x" />
                    </footer>
                </form>
            </field>
        </record>

        <record id="distributor_achievement_wizard_view" model="ir.actions.act_window">
            <field name="name">Distributor Achievement</field>
            <field name="res_model">distributor.achievement.wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

</odoo>
